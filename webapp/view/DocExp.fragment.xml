<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		noDataText="No hi han documents"
		title="Selecciona documents"
		confirm="handleDocClose"
		items="{ExpToDoc}">
		<StandardListItem
			title="{Filename}"
			description="{Mimetype}"
			info="{Guid}"
			type="Active" />
	</SelectDialog>
</core:FragmentDefinition>